<AnchorPane prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scenes.AdminDiscountController">
   <children>
      <ChoiceBox fx:id="choices" layoutX="225.0" layoutY="93.0" prefWidth="150.0" />
      <Label layoutX="225.0" layoutY="60.0" prefHeight="26.0" prefWidth="97.0" text="Discount Type">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="discountRat" layoutX="225.0" layoutY="149.0" promptText="Discount Ratio" />
      <Button layoutX="273.0" layoutY="295.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <ImageView fx:id="backImage" fitHeight="18.0" fitWidth="28.0" layoutX="25.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@/scenes/img/arrow.png" />
         </image>
      </ImageView>
      <Label fx:id="result" layoutX="244.0" layoutY="264.0" prefHeight="18.0" prefWidth="111.0" />
      <GridPane fx:id="serviceName" layoutX="250.0" layoutY="197.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </children>
</AnchorPane>
