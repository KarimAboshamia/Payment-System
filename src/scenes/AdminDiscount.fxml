            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="discountRat" layoutX="225.0" layoutY="149.0" promptText="Discount Ratio" />
      <Button layoutX="274.0" layoutY="220.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <ImageView fx:id="backImage" fitHeight="18.0" fitWidth="28.0" layoutX="25.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../arrow.png" />
